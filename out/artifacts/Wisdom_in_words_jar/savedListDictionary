ие","isDifficult":false},{"learnedEn":false,"en":"benefit","learnedRu":false,"ru":"польза, выгода/приносить пользу","isDifficult":false},{"learnedEn":false,"en":"benevolent","learnedRu":false,"ru":"великодушный или благожелательный","isDifficult":false},{"learnedEn":false,"en":"berry","learnedRu":false,"ru":"ягода","isDifficult":false},{"learnedEn":false,"en":"beside","learnedRu":false,"ru":"рядом","isDifficult":false},{"learnedEn":false,"en":"besides","learnedRu":false,"ru":"кроме","isDifficult":false},{"learnedEn":false,"en":"bet","learnedRu":false,"ru":"пари/держать пари","isDifficult":false},{"learnedEn":false,"en":"betray","learnedRu":false,"ru":"предавать, обманывать","isDifficult":false},{"learnedEn":false,"en":"beware","learnedRu":false,"ru":"остерегаться","isDifficult":false},{"learnedEn":false,"en":"beyond","learnedRu":false,"ru":"за, по ту сторону, вне","isDifficult":false},{"learnedEn":false,"en":"bias","learnedRu":false,"ru":"склонять или оказывать влияние/уклон или предубеждение","isDifficult":false},{"learnedEn":false,"en":"Bible","learnedRu":false,"ru":"Библия","isDifficult":false},{"learnedEn":false,"en":"bicycle","learnedRu":false,"ru":"велосипед","isDifficult":false},{"learnedEn":false,"en":"bid","learnedRu":false,"ru":"приглашать, предлагать","isDifficult":false},{"learnedEn":false,"en":"bill","learnedRu":false,"ru":"счёт к оплате","isDifficult":false},{"learnedEn":false,"en":"bill","learnedRu":false,"ru":"текст закона, законопроект","isDifficult":false},{"learnedEn":false,"en":"billboard","learnedRu":false,"ru":"доска объявлений, афиша","isDifficult":false},{"learnedEn":false,"en":"billion","learnedRu":false,"ru":"миллиард","isDifficult":false},{"learnedEn":false,"en":"bind","learnedRu":false,"ru":"связывать, привязывать","isDifficult":false},{"learnedEn":false,"en":"birch","learnedRu":false,"ru":"берёза","isDifficult":false},{"learnedEn":false,"en":"bird","learnedRu":false,"ru":"птица","isDifficult":false},{"learnedEn":false,"en":"birth","learn